<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <!-- ==================== AmmoSet ========================== -->

	<CombatExtended.AmmoSetDef>
		<defName>AmmoSet_FT_132mmM13</defName>
		<label>132mm M13 rockets</label>
		<ammoTypes>
			<Ammo_132mmM13_HE>Bullet_FT_132mmM13_HE</Ammo_132mmM13_HE>
		</ammoTypes>
		<isMortarAmmoSet>true</isMortarAmmoSet>
	</CombatExtended.AmmoSetDef>
	
	<CombatExtended.AmmoSetDef>
		<defName>AmmoSet_FT_132mmM13_arty</defName>
		<label>132mm M13 rockets</label>
		<ammoTypes>
			<Ammo_132mmM13_HE>Bullet_FT_132mmM13_HE_arty</Ammo_132mmM13_HE>
		</ammoTypes>
		<isMortarAmmoSet>true</isMortarAmmoSet>
	</CombatExtended.AmmoSetDef>

  <!-- ================== Projectiles ================== -->
  
	<!-- indirect fire (on map)-->
	
	<ThingDef Name="BaseFTBase132mmM13_HE" Class="CombatExtended.AmmoDef" ParentName="Base130mmType63">
		<defName>Bullet_FT_132mmM13_HE_arty</defName>
		<description>132mm heavy high explosive fragmentation rocket typically used in multi-rail rocket artillery launchers.</description>
		<thingClass>CombatExtended.ProjectileCE_Explosive</thingClass>
		<label>132mm M13 rocket (HE-Frag)</label>
		<graphicData>
			<texPath>Things/Projectile/Rocket/132mmM13</texPath>
			<graphicClass>Graphic_Single</graphicClass>
		</graphicData>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<speed>65</speed>
			<damageDef>Bomb</damageDef>
			<damageAmountBase>337</damageAmountBase>
			<explosionRadius>4</explosionRadius>
			<applyDamageToExplosionCellsNeighbors>true</applyDamageToExplosionCellsNeighbors>
			<flyOverhead>true</flyOverhead>
			<gravityFactor>25</gravityFactor>	<!-- This makes it so it doesn't shoot up too much vertically, however has weird inconsistent trajectory sometimes so commented out for now -->
			<spreadMult>1.3</spreadMult>
		</projectile>
		<comps>
			<li Class="CombatExtended.CompProperties_Fragments">
				<fragments>
					<Fragment_Large>43</Fragment_Large>
					<Fragment_Small>67</Fragment_Small>
				</fragments>
			</li>
		</comps>
	</ThingDef>
	
	<!-- indirect fire (off-map)-->
	
	<ThingDef ParentName="BaseFTBase132mmM13_HE">
		<defName>Bullet_FT_132mmM13_HE</defName>
		<projectile Class="CombatExtended.ProjectilePropertiesCE">
			<flyOverhead>true</flyOverhead>
			<speed>0</speed>
			<shellingProps>
				<iconPath>Things/WorldObjects/Munitions/Rocket</iconPath>
				<tilesPerTick>0.12</tilesPerTick>
				<range>20</range>
				<damage>0.23</damage>
			</shellingProps>
		</projectile>
	</ThingDef>
	
</Defs>